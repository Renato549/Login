<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="#302E3B">
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="421dp"
        android:layout_height="60dp"
        android:background="#1D1C23"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-11dp"
        tools:layout_editor_absoluteY="-1dp">

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="40dp"
            android:layout_height="38dp"
            android:adjustViewBounds="false"
            android:background="#00FFFFFF"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/toppng_com_menu_icon_png_3_lines_2000x2000"
            tools:layout_editor_absoluteX="1dp"
            tools:layout_editor_absoluteY="14dp" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="187dp"
            android:layout_height="36dp"
            app:srcCompat="@drawable/captura1"
            tools:layout_editor_absoluteX="76dp"
            tools:layout_editor_absoluteY="16dp" />

    </androidx.appcompat.widget.Toolbar>
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar2"
        android:layout_width="421dp"
        android:layout_height="60dp"
        android:background="#1D1C23"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-10dp"
        tools:layout_editor_absoluteY="675dp"/>


    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Estado: Boleto Usado"
        android:textColor="@color/white"
        android:textSize="30dp"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="586dp" />



    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="378dp"
        android:layout_height="679dp"
        app:srcCompat="@drawable/conciertos_1_1"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="-163dp" />
    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bungus"
        android:textColor="@color/teal_200"
        android:textSize="30dp"
        tools:layout_editor_absoluteX="155dp"
        tools:layout_editor_absoluteY="103dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5 de Septiembre"
        android:textColor="@color/teal_200"
        android:textSize="30dp"
        tools:layout_editor_absoluteX="96dp"
        tools:layout_editor_absoluteY="166dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/galaxy_s21_ultra_precio"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="328dp" />
</androidx.constraintlayout.widget.ConstraintLayout>